<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
    <servlet-name>helloServlet</servlet-name>  <!-- any name -->
    <servlet-class>ro.sci.servlet.HelloWorldServlet</servlet-class>
  
  </servlet>

    <servlet>
        <servlet-name>userServlet</servlet-name>  <!-- any name -->
        <servlet-class>ro.sci.servlet.UserServlet</servlet-class>

    </servlet>

  <servlet>
    <servlet-name>calculateAge</servlet-name>  <!-- any name -->
    <servlet-class>ro.sci.servlet.CalculateAgeServlet</servlet-class>

  </servlet>

  <servlet-mapping>
    <servlet-name>helloServlet</servlet-name> <!-- the same name as in the definition-->
    <url-pattern>/say-hello</url-pattern>
  </servlet-mapping>


  <servlet-mapping>
    <servlet-name>userServlet</servlet-name> <!-- the same name as in the definition-->
    <url-pattern>/users</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>calculateAge</servlet-name> <!-- the same name as in the definition-->
    <url-pattern>/myAge</url-pattern>
  </servlet-mapping>

</web-app>
